{
  "name": "@moominbot/moomin",
  "version": "0.1.0",
  "description": "It's a Bird?? It's a Plane?? It's Moomin bot",
  "main": "build/index.js",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "rimraf build && tsc",
    "build:check": "tsc --noEmit",
    "dev": "cross-env NODE_ENV=development npm run build && node build/index.js",
    "start": "cross-env NODE_ENV=production npm run build && node build/index.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write src/**/*.{js,ts,jsx,tsx,json}",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MoominBot/MoominBot.git"
  },
  "author": "MoominBot Developers",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/MoominBot/MoominBot/issues"
  },
  "homepage": "https://github.com/MoominBot/MoominBot#readme",
  "devDependencies": {
    "@types/common-tags": "^1.8.1",
    "@types/node": "^16.11.9",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.3.0",
    "husky": "^7.0.4",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "@discordjs/rest": "^0.1.0-canary.0",
    "bufferutil": "^4.0.5",
    "common-tags": "^1.8.2",
    "cross-env": "^7.0.3",
    "dayjs": "^1.10.7",
    "discord-api-types": "^0.24.0",
    "discord.js": "^13.3.1",
    "dotenv": "^10.0.0",
    "erlpack": "discord/erlpack",
    "readdirp": "^3.6.0",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.6.0",
    "utf-8-validate": "^5.0.7",
    "winston": "^3.3.3",
    "zlib-sync": "^0.1.7"
  },
  "engines": {
    "node": ">=16.1.0"
  }
}
